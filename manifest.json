{
    "manifest_version": 2,
    "name": "Word Definition Finder",
    "version": "0.1",
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["main.js"]
      }
    ],
    "background": {
      "scripts": ["background.js"]
    },
    "browser_action": {
      "default_popup": "sketch/index.html",
      "default_title": "Word Definition Finder"
    }
  }